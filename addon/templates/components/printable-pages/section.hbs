{{#if (and this.shouldRender this.page)}}
  <div class='PrintablePages-section' style={{{this.style}}}>
    {{#each this.items as |item index|}}
      {{#printable-pages/section-item section=this.section}}
        {{yield (hash
          data=item
          index=index
        )}}
      {{/printable-pages/section-item}}
    {{/each}}
  </div>
{{/if}}
